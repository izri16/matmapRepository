<?xml version="1.0" encoding="utf-8"?>
<resources>

    <!-- ActionBar styles -->
    <style name="MyActionBar" parent="@style/Widget.AppCompat.Light.ActionBar.Solid.Inverse">
        <item name="android:background">#1E7E79</item>
        <item name="android:textColor">#FFFFFF</item>

        <!-- Support library compatibility -->
        <item name="background">@drawable/back2</item>
    </style>

    <!-- Main Button nine-patch style -->
    <style name="MyCustomButton" parent="android:Widget.Button">
        <item name="android:background">@drawable/btn_default_normal_lblue</item>
    </style>

    <!-- Secondary Button nine-patch style -->
    <style name="MySecondaryButton" parent="android:Widget.Button">
        <item name="android:background">@drawable/btn_default_normal_lblue</item>
    </style>

</resources>
